Algoritmo "SalarioFuncionaria"


Var
   funcionaria: caractere
   Salario: real

Inicio
      Escreva ("Digite seu nome: ")
      Leia (funcionaria)
      Escreva ("Digite seu salário: ")
      Leia (Salario)
      
      Escreva ("O funcionário ", funcionaria, " recebe o salário de", Salario)

Fimalgoritmo