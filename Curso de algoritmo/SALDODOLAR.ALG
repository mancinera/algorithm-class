Algoritmo "SaldoDolar"

Var
    carteira, cotacao_dolar: real

Inicio
      Escreva ("Qual a quantidade em R$ você possui em sua carteira? ")
      Leia(carteira)
      cotacao_dolar <- (5.09)
      carteira <- carteira / saldo_dolar
      Escreva ("A quantidade em dólares que poderá comprar é de ", carteira:4:2)

Fimalgoritmo