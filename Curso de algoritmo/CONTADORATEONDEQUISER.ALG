Algoritmo "ContadorAteOndeQuiser"

Var
   valor, contador: inteiro
Inicio
      contador <- 0
      Escreva ("Quer contar até quanto? ")
      Leia(valor)
      Enquanto (contador <=valor) faça
              Escreval (contador)
              contador <- contador +1
      FimEnquanto
      Escreval ("Terminei de contar. ")
Fimalgoritmo